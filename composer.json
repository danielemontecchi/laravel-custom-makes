{
  "name": "danielemontecchi/laravel-custom-makes",
  "description": "Create custom make commands in Laravel using a CLI wizard",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Daniele Montecchi",
      "email": "you@example.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "illuminate/console": "^10.0|^11.0|^12.0",
    "illuminate/support": "^10.0|^11.0|^12.0"
  },
  "autoload": {
    "psr-4": {
      "DanieleMontecchi\\CustomMakes\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "DanieleMontecchi\\CustomMakes\\CustomMakesServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "pestphp/pest": "^3.8",
    "orchestra/testbench": "^10.2"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
